fn main() {
    printLn("Enter_a_number:");
    n = getOneInt();
    out = if n == 5 then "Is_Equal_to_5" else processFactorial(n);
    printLn(out);
    return 0; 
}

fn processFactorial(n) {
    fact = factorial(n);
    printLn(fact);
    return "Is_not_Equal_to_5";
}

fn factorial(n) {
    ans = if n == 0 then 1 else n * factorial(n-1);
    return ans;
}